" vim: set ft=vimperator:

map J <C-d>
map K <C-u>
 
map <C-`> :stop<CR>
cmap <C-`> :stop<CR>
 
cmap <S-Return> <End>.org<Home><C-Right><Right>www.<CR>
cmap <C-Return> <End>.com<Home><C-Right><Right>www.<CR>

set gui=none
set gui=tabs
